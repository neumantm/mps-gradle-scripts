<!-- DUMMY Build.xml to demonstrate the ANT calls from Gradle -->
<project name="mps-gradle-scripts" default="build">
  <!-- Generated by MPS -->

  <target name="assemble" depends="classes, declare-mps-tasks">
    <echo message="assemble" />
  </target>
  
  <target name="buildDependents" />
  
  <target name="fetchDependencies" />
  
  <target name="build" depends="assemble" />
  
  <target name="clean">
    <echo message="clean" />
  </target>
  
  <target name="compileJava" />
  
  <target name="processResources" />
  
  <target name="classes" depends="compileJava, processResources" />
  
  <target name="test" depends="classes, module-tests" />
  
  <target name="check" depends="test" />
  
  <target name="generate" depends="declare-mps-tasks, fetchDependencies">
    <echo message="generate" />
  </target>
  
  <target name="declare-mps-tasks">
    <echo message="declare-mps-tasks" />
  </target>
  
  <target name="makeDependents" />
  
  <target name="module-tests" depends="assemble, test.runModuleTests" />

  <target name="test.runModuleTests" depends="assemble">
    <echo message="test.runModuleTests" />
  </target>

  <target name="cleanSources">
    <echo message="cleanSources" />
  </target>
</project>